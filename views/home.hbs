 <header class="header-dark" style="height: 94.625px;background: url(&quot;assets/img/bg.jpg&quot;);padding-bottom: 0px;">
        <nav class="navbar navbar-dark navbar-expand-lg navigation-clean-search" style="padding-bottom: 0px;padding-top: 9px;">
            <div class="container"><a class="navbar-brand" href="home" style="font-size: 43px;">Tinder</a><button data-toggle="collapse" class="navbar-toggler" data-target="#navcol-1"><span class="sr-only">Toggle navigation</span><span class="navbar-toggler-icon"></span></button>
                <div class="collapse navbar-collapse" id="navcol-1">
                    <form class="form-inline mr-auto" target="_self">
                        <div class="form-group"><label for="search-field"></label></div>
                    </form><span class="navbar-text"><a class="login" href="#" style="color: rgb(255,255,255);font-size: 20px;">CÀI ĐẶT</a></span><a class="btn btn-light action-button" role="button" href="/" style="border-radius: 10px;width: 126.2375px;height: 47px;padding-top: 12px;background: rgb(255,255,255);color: rgb(255,0,0);"><strong>ĐĂNG</strong>&nbsp;<strong>XUẤT</strong></a>
                </div>
            </div>
        </nav>
    </header>
    <div class="container" style="margin-top: 0px;color: rgb(47,52,56);">
        <div class="table-responsive" style="margin-top: 60px">
            <table class="table">
                <thead>
                    <tr>
                        <form action="/insertUser" enctype="multipart/form-data" method="post">
                        <th><button class="btn btn-primary" type="submit" style="background: #851e3e;">THÊM</button></th>
                        <th><input name="avatar" type="file" style="text-align: center;" required></th>
                        <th><input name="username" type="text" style="text-align: center; border-radius:50px" placeholder="Họ và Tên" required></th>
                        <th><input name="dob" type="text" style="text-align: center;border-radius:50px" placeholder="Ngày Sinh" required></th>
                        <th><input name="emailsdt" type="emailsdt" style="text-align: center;border-radius:50px" placeholder="Email/SĐT" required></th>
                        <th><input name="gender" type="gender" style="text-align: center;border-radius:50px" placeholder="Giới tính" required></th>
                        <th><input name="hobit" type="hobit" style="text-align: center;border-radius:50px" placeholder="Sở thích" required></th>
                        <th><input name="description" type="description" style="text-align: center;border-radius:50px" placeholder="Giới thiệu ngắn" required></th>
                        </form>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td></td>
                        <td><strong>AVATAR</strong></td>
                        <td><strong>HỌ VÀ TÊN</strong></td>
                        <td><strong>NGÀY SINH</strong></td>
                        <td><strong>EMAIL/SĐT</strong></td>
                        <td><strong>GIỚI TÍNH</strong></td>
                        <td><strong>SỞ THÍCH</strong></td>
                        <td><strong>GIỚI THIỆU NGẮN</strong></td>
                    </tr>
                        {{#each users}}
                            <tr>
                            <td>
                                <form action="/{{this._id}}" enctype="multipart/form-data" method="get">
                                    <button class="btn btn-primary" type="submit" style="background: #005b96;width: 68.2125px;margin-bottom: 10px;">SỬA&nbsp;</button>
                                </form>
                                <form action="/{{this._id}}" enctype="multipart/form-data" method="post">
                                <button class="btn btn-primary" type="submit" style="background: #fe4a49;width: 68.2125px;">XÓA</button>
                                </form>
                            </td>
                            <td><img style="width: 100px;height: 100px;border-radius: 100px" src="./assets/img/{{this.avatar}} "/></td>
                            <td>{{this.username}}</td>
                            <td>{{this.dob}}</td>
                            <td>{{this.emailsdt}}</td>
                            <td>{{this.gender}}</td>
                            <td>{{this.hobit}}</td>
                            <td>{{this.description}}</td>
                            </tr>
                        {{/each}}
                </tbody>
            </table>
        </div>
    </div>
